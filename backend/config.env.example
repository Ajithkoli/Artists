# ArchiCanvas Backend Configuration Template
# Populate these values in your config.env file

# MongoDB connection string from Atlas
DB_URL=mongodb+srv://<username>:<password>@cluster0.mongodb.net/archicanvas?retryWrites=true&w=majority

# Cloudinary Credentials (from https://cloudinary.com/console)
CLOUDINARY_CLOUD_NAME=your_cloud_name
CLOUDINARY_API_KEY=your_api_key
CLOUDINARY_API_SECRET=your_api_secret

# Other existing keys
JWT_SECRET=your_secret_key
RAZORPAY_KEY_ID=your_razorpay_id
RAZORPAY_KEY_SECRET=your_razorpay_secret

# AI Keys (if used)
HUGGING_FACE_TOKEN=your_token
GOOGLE_API_KEY=your_google_key
